{
	"name": "BinarySrc",
	"properties": {
		"linkedServiceName": {
			"referenceName": "manitesting_raw",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			},
			"file_extension": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('*',dataset().file_extension)",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			}
		}
	}
}